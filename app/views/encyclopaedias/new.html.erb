<div id ="encyclopaedia">

<h1>New Article</h1>

<% form_for(@encyclopaedia) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :intro %><br />
    <%= f.text_area :intro %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', encyclopaedias_path %>
</div>